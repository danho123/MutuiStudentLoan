<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.corgipark.mutui.car"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/mutui_blue" >
        <activity
            android:name="com.corgipark.activity.MutuiCars"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.corgipark.activity.PaymentActivity"
            android:label="@string/title_activity_payment" >
        </activity>
        <activity
            android:name="com.corgipark.activity.SummaryActivity"
            android:label="@string/title_activity_summary" >
        </activity>
        <activity
            android:name="com.corgipark.activity.AmortizationActivity"
            android:label="@string/title_activity_amortization" />
        <activity android:name="org.achartengine.GraphicalActivity" />
        <activity
            android:name="com.corgipark.activity.XYChartBuilder"
            android:label="@string/title_activity_amortization" />
        <activity
            android:name="com.corgipark.activity.LoanListActivity"
            android:label="@string/title_activity_loan_list" >
        </activity>
        <activity
            android:name="com.corgipark.activity.SettingsActivity"
            android:label="@string/title_activity_settings" >
        </activity>
        <activity
            android:name="com.corgipark.activity.EconomyActivity"
            android:label="@string/title_activity_economy" >
        </activity>
        <activity
            android:name="com.corgipark.activity.AmortizationScheduleActivity"
            android:label="@string/title_activity_amortization_schedule" >
        </activity>
        <activity
            android:name="com.corgipark.activity.AffordabilityActivity"
            android:label="@string/title_activity_affordability_fragment" >
        </activity>
    </application>

</manifest>