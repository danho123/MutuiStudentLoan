	<LinearLayout android:id="@+id/chart" android:orientation="horizontal"
		android:layout_width="fill_parent" android:layout_height="0dp" android:layout_weight=".65" />
    <LinearLayout android:id="@+id/chart2" android:orientation="horizontal"
        android:layout_width="fill_parent" android:layout_height="0dp" android:layout_weight=".65" />
	<LinearLayout
	    android:layout_width="fill_parent"
	    android:layout_height="0dp"
	    android:layout_weight="0.1" >

	    <Button
	        android:id="@+id/aGraph_button_aSchedule"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:onClick="onClick_schedule"
	        android:text="Schedule" />
	</LinearLayout>

</LinearLayout>
